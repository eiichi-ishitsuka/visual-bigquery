{
    "name": "visual-bigquery",
    "publisher": "eiichi-ishitsuka",
    "repository": {
        "type": "git",
        "url": "git://github.com/eiichi-ishitsuka/visual-bigquery.git"
    },
    "icon": "asset/icon.png",
    "displayName": "Visual-BigQuery",
    "description": "Syntax Highlight Google BigQuery SQL",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension.js",
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "contributes": {
        "languages": [
            {
                "id": "sql",
                "aliases": [
                    "BigQuery SQL",
                    "sql"
                ],
                "extensions": [
                    ".sql"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "sql",
                "scopeName": "source.bigquery.sql",
                "path": "./syntaxes/sql.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "sql",
                "path": "./snippets/functions.code-snippets"
            },
            {
                "language": "sql",
                "path": "./snippets/idioms.code-snippets"
            }
        ]
    },
    "dependencies": {
        "@types/vscode": "^1.58.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "14.x",
        "eslint": "^7.27.0",
        "glob": "^7.1.7",
        "mocha": "^8.4.0",
        "typescript": "^4.3.2",
        "vscode-test": "^1.5.2",
        "vsce": "^1.96.1"
    }
}
